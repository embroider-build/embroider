{
  "name": "@embroider/test-setup",
  "version": "3.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/embroider-build/embroider.git",
    "directory": "packages/test-setup"
  },
  "license": "MIT",
  "main": "src/index.js",
  "files": [
    "src/index.js"
  ],
  "scripts": {
    "build": "concurrently 'npm:build:*'",
    "build:js": "rollup -c ./rollup.config.mjs",
    "build:types": "build-types",
    "start": "pnpm build:js --watch",
    "prepack": "pnpm -w exec turbo --filter '@embroider/test-setup' build"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "resolve": "^1.20.0"
  },
  "devDependencies": {
    "@embroider/build": "workspace:*",
    "@embroider/compat": "workspace:^",
    "@embroider/core": "workspace:^",
    "@embroider/webpack": "workspace:^",
    "@types/lodash": "^4.14.170"
  },
  "peerDependencies": {
    "@embroider/compat": "workspace:^",
    "@embroider/core": "workspace:^",
    "@embroider/webpack": "workspace:^"
  },
  "peerDependenciesMeta": {
    "@embroider/compat": {
      "optional": true
    },
    "@embroider/core": {
      "optional": true
    },
    "@embroider/webpack": {
      "optional": true
    }
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
